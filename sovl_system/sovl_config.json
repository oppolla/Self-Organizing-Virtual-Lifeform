  },
  "memory_config": {
    "memoria": {
      "storage_path": "storage/",
      "max_storage_mb": 5120,
      "state_save_interval": 300,
      "compression_enabled": true,
      "encryption_enabled": false
    },
    "ram": {
      "max_memory_mb": 1024,
      "memory_threshold": 0.8,
      "memory_decay_rate": 0.95,
      "dream_memory_maxlen": 10,
      "conversation_history_maxlen": 20
    },
    "gpu": {
      "max_memory_mb": 2048,
      "memory_threshold": 0.9,
      "check_interval": 1000,
      "batch_size_adjustment": 0.5,
      "clear_cache_threshold": 0.95
    }
  },
  "state_config": {
    "max_history": 500,
    "state_file": "sovl_state.json"
  },
  "confidence_config": {
    "history_maxlen": 5,
    "weight": 0.5
  },
  "dream_memory_config": {
    "max_memories": 100,
    "base_weight": 0.1,
    "max_weight": 1.5
  },
  "gestation_weighting": {
    "metadata_fields": {
      "quality_metrics": {
        "code": {
          "enabled": true,
          "weight": 1.5
        },
        "url": {
          "enabled": true,
          "weight": 1.3
        },
        "question": {
          "enabled": true,
          "weight": 1.2
        },
        "exclamation": {
          "enabled": true,
          "weight": 1.1
        },
        "emoji": {
          "enabled": true,
          "weight": 0.9
        }
      },
      "content_metrics": {
        "word_count": 0,
        "context_word_count": 0,
        "avg_word_length": 0.0,
        "sentence_count": 0,
        "avg_sentence_length": 0.0
      },
      "confidence_metrics": {
        "confidence": 0.5,
        "temperament": 0.5
      },
      "context_metrics": {
        "is_first_message": false,
        "is_last_message": false,
        "context_window_size": 0,
        "response_time": 0.0
      }
    },
    "content_weights": {
      "word_count_ratio_scale": 0.5,
      "optimal_word_length_range": {
        "min": 4,
        "max": 8,
        "weight": 1.2
      },
      "suboptimal_word_length_weight": 0.8,
      "optimal_sentence_count_range": {
        "min": 2,
        "max": 5,
        "weight": 1.2
      },
      "excessive_sentence_weight": 0.9,
      "optimal_sentence_length_range": {
        "min": 10,
        "max": 20,
        "weight": 1.2
      },
      "excessive_sentence_length_weight": 0.9
    },
    "confidence_weights": {
      "high_confidence_threshold": 0.8,
      "high_confidence_weight": 1.3,
      "moderate_confidence_threshold": 0.6,
      "moderate_confidence_weight": 1.1,
      "low_confidence_threshold": 0.4,
      "low_confidence_weight": 0.8
    },
    "temperament_weights": {
      "balanced_range": {
        "min": 0.4,
        "max": 0.6,
        "weight": 1.2
      },
      "extreme_temperament_weight": 0.9
    },
    "context_weights": {
      "first_last_message_weight": 1.2,
      "optimal_context_range": {
        "min": 2,
        "max": 4,
        "weight": 1.1
      },
      "excessive_context_weight": 0.9
    },
    "timing_weights": {
      "max_response_time": 60,
      "optimal_timing_weight": 1.0
    },
    "weight_bounds": {
      "min": 0.1,
      "max": 3.0
    }
  }
